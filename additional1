#include <iostream>
using namespace std;

void countingSort(int arr[], int n) {
    int maxVal = arr[0];
    for (int i = 1; i < n; i++)
        if (arr[i] > maxVal)
            maxVal = arr[i];

    int count[maxVal + 1] = {0};

    for (int i = 0; i < n; i++)
        count[arr[i]]++;

    int k = 0;
    for (int i = 0; i <= maxVal; i++) {
        while (count[i] > 0) {
            arr[k++] = i;
            count[i]--;
        }
    }
}

int main() {
    int arr[] = {4, 2, 2, 8, 3, 3, 1};
    int n = 7;

    countingSort(arr, n);

    for (int i = 0; i < n; i++)
        cout << arr[i] << " ";
}
